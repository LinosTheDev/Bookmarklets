javascript:(function() {  var newTitle = prompt("Enter the new tab title:");  var newFavicon;  if (newTitle !== null) {    var option = prompt("Choose an option: \n1: Google Search \n2: Google Drive \n3: Custom");    switch (option) {      case "1":        newFavicon = "https://www.google.com/favicon.ico";        break;      case "2":        newFavicon = "https://drive.google.com/favicon.ico";        break;      case "3":        var customURL = prompt("Enter the custom URL (without 'https://' or 'www.'):").trim();        if (customURL !== null) {          var faviconURL = getFaviconURL(customURL);          if (faviconURL !== null) {            newFavicon = faviconURL;          } else {            alert("Invalid URL or unable to retrieve favicon.");          }        } else {          return;        }        break;      default:        alert("Invalid option selected.");        return;    }    if (newFavicon !== undefined) {      setFavicon(newFavicon);      document.title = newTitle;    }  }  function setFavicon(faviconURL) {    var link = document.querySelector("link[rel*='icon']") || document.createElement("link");    link.type = "image/x-icon";    link.rel = "shortcut icon";    link.href = faviconURL;    document.getElementsByTagName("head")[0].appendChild(link);  }  function getFaviconURL(url) {    if (!url.startsWith("http")) {      url = "https://www." + url;    }    var parser = document.createElement("a");    parser.href = url;    var hostname = parser.hostname;    return "https://" + hostname + "/favicon.ico";  }})();
