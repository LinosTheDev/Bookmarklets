javascript:(function() {
  var choice = prompt("Choose an option by entering the corresponding number:\n1: Incognito\n2: Hypertabs\n3: Holy Unblocker\n4: Noodle\n5: AboutBrowser\n6: Immortal\n7: Other\n8: Submit");
  
  if (choice === "1") {
    window.location.href = "https://example-site-incognito.com";
  } else if (choice === "2") {
    window.location.href = "https://example-site-hypertabs.com";
  } else if (choice === "3") {
    window.location.href = "https://example-site-holyunblocker.com";
  } else if (choice === "4") {
    window.location.href = "https://example-site-noodle.com";
  } else if (choice === "5") {
    window.location.href = "https://example-site-aboutbrowser.com";
  } else if (choice === "6") {
    window.location.href = "https://example-site-immortal.com";
  } else if (choice === "7") {
    window.location.href = "https://example-site-other.com";
  } else if (choice === "8") {
    window.location.href = "https://www.amazon.com";
  } else {
    alert("Invalid choice. Please select a valid option.");
  }
})();
